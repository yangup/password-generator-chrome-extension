{
    "name": "Password Generator",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Password Generator",
    "icons": {
        "16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
		"default_title": "Password Generator",
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "matches": ["http://www.google.com/*"],
            "js": [
                "script.js"
            ],
			"run_at":"document_start"
        }
    ]
}