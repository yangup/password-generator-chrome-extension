{
  "name": "Password Generator",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Password Generator",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Password Generator",
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "js/script.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "js/script.js"
      ],
      "run_at": "document_start"
    }
  ]
}